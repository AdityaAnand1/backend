{
  "name": "@reacher/microservices",
  "version": 2,
  "alias": ["reacher.now.sh"],
  "env": {
    "MONGODB_ATLAS_URI": "@reacher-mongodb-atlas-uri",
    "SERVERLESS_VERIFIER_ENDPOINT": "@reacher-serverless-verifier-endpoint"
  },
  "routes": [
    {
      "src": "/api/user",
      "methods": ["GET", "OPTIONS"],
      "dest": "/api/user.ts"
    },
    {
      "src": "/api/verify/demo",
      "dest": "/api/verify/demo.ts",
      "methods": ["GET"]
    }
  ]
}
